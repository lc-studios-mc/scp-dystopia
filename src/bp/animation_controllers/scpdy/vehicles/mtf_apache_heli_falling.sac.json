{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.scpdy.bp.mtf_apache_heli_falling.main": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": ["anim.fall"],
					"transitions": [
						{
							"explode": "q.all_animations_finished || q.is_on_ground"
						}
					]
				},
				"explode": {
					"on_entry": ["@s explode"]
				}
			}
		}
	}
}
