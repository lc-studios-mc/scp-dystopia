{
	"format_version": "1.8.0",
	"animations": {
		"animation.scpdy.sentry_turret.look": {
			"loop": true,
			"bones": {
				"yaw": {
					"rotation": [0, "q.body_y_rotation + v.look_y", 0]
				},
				"pitch": {
					"rotation": ["v.look_x", 0, 0]
				},
				"root": {
					"rotation": [0, "-q.body_y_rotation", 0]
				}
			}
		},
		"animation.scpdy.sentry_turret.gun_roll": {
			"loop": true,
			"animation_length": 1,
			"anim_time_update": "v.gun_roll_anim_time ?? q.life_time",
			"bones": {
				"roll": {
					"rotation": {
						"0.0": [0, 0, 0],
						"1.0": [0, 0, 360]
					}
				}
			}
		},
		"animation.scpdy.sentry_turret.broken": {
			"loop": "hold_on_last_frame",
