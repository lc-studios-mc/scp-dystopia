{
  "format_version": "1.21.40",
  "minecraft:entity": {
    "description": {
      "identifier": "lc:scpdy_unknown_core_meteorite",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "runtime_identifier": "minecraft:snowball",
      "properties": {
        "lc:state": {
          "type": "int",
          "range": [0, 3],
          "default": 0,
          "client_sync": true
        }
      }
    },
    "component_groups": {
      "ascending": {
        "minecraft:timer": {
          "looping": false,
          "randomInterval": false,
          "time": 1.5,
          "time_down_event": {
            "event": "unknown_core_meteorite:complete_ascending",
            "target": "self"
          }
        }
      },
      "ascended": {
        "minecraft:timer": {
          "looping": false,
          "randomInterval": true,
          "time": [0.8, 0.9],
          "time_down_event": {
            "event": "unknown_core_meteorite:start_fall_delay_timer",
            "target": "self"
          }
        }
      },
      "fall_delay_timer": {
        "minecraft:timer": {
          "looping": false,
          "randomInterval": false,
          "time": 0.5,
          "time_down_event": {
            "event": "unknown_core_meteorite:start_falling",
            "target": "self"
          }
        }
      },
      "falling": {
        "minecraft:timer": {
          "looping": false,
          "randomInterval": false,
          "time": 0.67,
          "time_down_event": {
            "event": "unknown_core_meteorite:complete_falling",
            "target": "self"
          }
        }
      },
      "minecraft:exploding": {
        "minecraft:explode": {
          "fuse_length": 0,
          "fuse_lit": true,
          "power": 2.0,
          "breaks_blocks": false
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": ["inanimate", "projectile", "unknown_core_fireball"]
      },
      "minecraft:health": {
        "value": 20,
        "max": 20
      },
      "minecraft:knockback_resistance": {
        "value": 1.0
      },
      "minecraft:breathable": {
        "breathes_lava": true,
        "breathes_solids": true,
        "breathes_water": true,
        "generates_bubbles": false
      },
      "minecraft:fire_immune": {},
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "all",
            "deals_damage": "no"
          }
        ]
      },
      "minecraft:collision_box": {
        "width": 0.1,
        "height": 0.1
      },
      "minecraft:physics": {
        "has_collision": true,
        "has_gravity": false
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:custom_hit_test": {
        "hitboxes": [
          {
            "width": 0.1,
            "height": 0.1,
            "pivot": [0, 100, 0]
          }
        ]
      },
      "minecraft:body_rotation_blocked": {},
      "minecraft:dimension_bound": {},
      "minecraft:persistent": {},
      "minecraft:game_event_movement_tracking": {
        "emit_flap": false,
        "emit_move": false,
        "emit_swim": false
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "trigger": "unknown_core_meteorite:start_ascending"
      },
      "unknown_core_meteorite:spawn_debug": {
        "trigger": "unknown_core_meteorite:start_ascending"
      },
      "unknown_core_meteorite:start_ascending": {
        "add": {
          "component_groups": ["ascending"]
        },
        "set_property": {
          "lc:state": 1
        }
      },
      "unknown_core_meteorite:complete_ascending": {
        "remove": {
          "component_groups": ["ascending"]
        },
        "add": {
          "component_groups": ["ascended"]
        },
        "set_property": {
          "lc:state": 2
        }
      },
      "unknown_core_meteorite:start_fall_delay_timer": {
        "remove": {
          "component_groups": ["ascended"]
        },
        "add": {
          "component_groups": ["fall_delay_timer"]
        }
      },
      "unknown_core_meteorite:start_falling": {
        "remove": {
          "component_groups": ["ascended"]
        },
        "add": {
          "component_groups": ["falling"]
        },
        "set_property": {
          "lc:state": 3
        }
      },
      "unknown_core_meteorite:complete_falling": {
        "remove": {
          "component_groups": ["falling"]
        },
        "trigger": "minecraft:explode"
      },
      "minecraft:explode": {
        "add": {
          "component_groups": ["minecraft:exploding"]
        }
      }
    }
  }
}
