{
	"[markdown]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"files.eol": "\n",
	"files.associations": {
		"*.json": "jsonc",
		"package.json": "json",
		"package-lock.json": "json"
	},
	"search.exclude": {
		"**/dist/**": true,
		"**/wt/**": true
	}
}
